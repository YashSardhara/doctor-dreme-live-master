    /* ----- Header Parallax ------- */
    body {
        &:not(.template-index) {
            .wrapper_header_parallax {
                // .wrapper-header-bt {
                //     @include box-shadow(0, 3px, 3px, -3px rgba(0,0,0,0.1));
