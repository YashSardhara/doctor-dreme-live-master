$product-regular: 'product_sansregular';
$primary-font: 'Roboto',
  sans-serif;
$border_widget_title : #e0e0e0;

@mixin transition($transition) {
  -webkit-transition: $transition;
  -moz-transition: $transition;
  -ms-transition: $transition;
  -o-transition: $transition;
  transition: $transition;
}

/* product tab page related css start here */
.product-item {
  background-color: $light-gray;
  padding: 20px;
  display: inline-block;
  width: 100%;
}

.page-product-tabs-sections {
  .main-title {
    font-size: 70px;
    line-height: 60px;
  }

  .product-tag-section {
    padding: 10px 0 70px;

    .nav {
      .nav-item {
        margin: 0 0 10px;

        .nav-link {
          font-size: 22px;
          color: $content-color;
          font-family: $primary-font;
          position: relative;
          padding: 0 0 0 15px;

          &::before {
            content: '';
            position: absolute;
            width: 4px;
            height: 100%;
            left: 0;
            top: 0;
            background-color: transparent;
          }

          &.active,
          &:hover {
            &::before {
              background-color: $primary-color;
            }
          }
        }
      }
    }

    .button-section {
      border-bottom: 1px solid $primary-color;
      padding-top: 30px;
      padding-bottom: 40px;
      margin: 0 15px 50px;
      max-width: calc(100% - 30px);
    }

    .product-grid-block:last-child {
      .button-section {
        display: none;
      }
    }
  }
}

