                display: none;
            }
        }
    }

    .wrapper_header_parallax {
        .sticky-wrapper {
            min-height: 82px;
        }

        .wrapper-navigation {
            position: static;
        }

        .is-sticky {
            .wrapper-header-bt {
                padding-top: 0;
                @include box-shadow(0, 3px, 3px, -3px rgba(0,0,0,0.1));
              	.header-logo{
                      padding: 10px 0;
                }
            }

            .site-nav {
                .menu-lv-1 {
                    > a {
                        padding-top: 20px;
                      	padding-bottom: 20px;
                    }
                }
            }

            .left-header{
                margin-top: 5px;
            }
          
          .right-header{
          	   margin-top: 15px;
          }
        }

        .wrapper-header-bt {
            position: relative;
            padding-top: 0;

            .header-logo {
                padding: 15px 0;
                  margin-top: 0px;
            }

            .header-panel-bt {
                @include flexbox();
                @include justify-content(space-between);
                @include align-items(flex-start);
            }

            .icon-search {
                svg {
                    top: 1px;
                }
            }
        }

        .left-header {
            padding-right: 15px;
            // width: 140px;
        }

        .right-header {
            @include flexbox();
            @include justify-content(flex-end);
            @include align-items(center);
            margin-top: 6px;

            > * {
                width: 45px;
                height: 30px;
                @include justify-content(flex-end);
            }

            .hamburger-icon {
                position: relative;
                top: 1px;
            }

            .icon-user {
                position: relative;
                top: -1px;
                padding: 3px 0;

                svg {
                    width: 22px;
                }
            }

            .wishlist-icon {
                padding-right: 3px;
            }

            .cart-icon {
                padding-right: 2px;
                position: relative;
                top: -1px;

                .cartCount {
                    right: -4px;
                    top: 0px;
                }
            }

            .nav-search {
                .search-form {
                    @include flexbox();
                    @include align-items(center);
                    position: absolute;
                    width: 100%;
                    z-index: 3;
                    height: 100%;
                    background-color: #f8f8f8;
                    visibility: hidden;
                    @include scale(0);
                    @include opacity(0);
                    @include transition(transform .5s ease);
                }

                .search-bar {
                    @include flexbox();
                    width: 1170px;
                    max-width: 82%;
                    margin: auto;
                    top: 0;
                    height: 100%;

                    .icon-search {
                        padding-right: 0;
                        @include justify-content(flex-end);
                        width: 52px;

                        svg {
                            width: 18px;
                            height: 18px;
                        }
                    }
                }

                .input-group-field {
                    font-size: $font_size + 4;
                    @include calc(width, '100% - 52px');
                    text-align: center;
                    padding-left: 64px;
                    line-height: $font_size + 11;
                }

                .close-search {
                    left: initial;
                    @include calc(right, '1.5% - 4px');
                    @include calc(top, '50% + 1px');
                    @include translateY(-50%);
                }
            }
        }

        .wrapper-navigation {
            margin-top: 0;
        }

        .site-nav {
            .menu-lv-1 {
                > a {
                    padding-bottom: 20px;
                  	padding-top: 20px;
                }

                .icon-label {
                    top: -4px;
                }
            }
        }
    }

