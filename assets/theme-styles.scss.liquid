@mixin filled_obj {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

@mixin label {
    font-size: $font_size;
    font-weight: 600;
    line-height: $font_size + 8;
    color: $page_title_color;
}

