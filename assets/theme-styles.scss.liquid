.list-categories {
    .grid-item {
        padding-bottom: 30px;

        .img-box {
            position: relative;

            &:hover {
                .view-abs {
                    @include opacity(1);
                    @include transform(translate(-50%, -50%) rotateX(0deg));
                }
            }
        }
    }

    .view-abs {
        position: absolute;
        top: 50%;
        left: 50%;
        @include transform(translate(-50%, -50%) rotateX(90deg));
        @include transition(transform .3s linear .1s);
        @include opacity(0);
        z-index: 2;

        .view {
            width: 40px;
            height: 40px;
            border: 1px solid $page_title_color;
            position: relative;
            display: block;
            @include transition(all .4s linear);

            &:before,
            &:after {
                position: absolute;
                content: "";
                top: 50%;
                left: 50%;
                @include translate(-50%, -50%);
                background-color: $page_title_color;
                @include transition(all .4s linear);
            }

            &:before {
                width: 2px;
                height: 20px;
            }

            &:after {
                width: 20px;
                height: 2px;
            }

            &:hover {
                background-color: $background_1;
                border-color: $background_1;

                &:before,
                &:after {
                    background-color: $color_1;
                }
            }
        }
    }
}

