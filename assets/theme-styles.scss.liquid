            .rte {
              display: inline-block;
              vertical-align: top;
            }
          }
        }
      
        .panel-title a {
          @include flexbox();
          @include align-items(flex-start);
          span {
              padding: 9px 25px 7px 12px;
            	
              &:before {
                  display: inline-block;
                  font-size: $font_size - 2;
                  font-weight: 500;
                  line-height: 1em;
                  padding: 4px 8px 3px;
                  min-width: 63px;
                  @include border-radius(1px);
                  margin-right: 20px;
                  text-align: center;
              }
          }
        }

        .panel-title {
            font-size: $font_size;
            line-height: $font_size + 12;
            font-weight: 500;
            margin-bottom: 0;

            a {
                @include border-radius(1px);
                padding-right: 65px;
                position: relative;

                span:before {
                    position: relative;
                    top: 0;
                }

                span:after {
                    display: inline-block;
                    font-size: $font_size;
                    font-weight: normal;
                    line-height: $font_size + 12;
                    text-align: right;
                    content: attr(data-show) "";
                    color: $breadcrumb_color;
                    position:absolute;
                    right: 25px;
                    top: 0;
                    padding: 9px 0 7px;
                    text-transform: capitalize;
                }

                &:not(.collapsed) {
                    span:after {
                        content: attr(data-hide) "";
                        color: #fff;
                    }
                }
            }
        }

        .panel-body {
            padding-top: 26px;
            padding-bottom: 10px;
            line-height: $font_size + 10;

            p {
                &:not(:last-child),
                &:not(:last-of-type) {
                    margin-bottom: 2rem;
                }
            }
        }
    }

    .contact-link {
        @include flexbox();
        @include justify-content(flex-end);
        @include align-items(center);

        .text {
            font-size: $font_size;
            line-height: $font_size + 12;
            margin-right: 20px;
        }

        .btn {
            @include button_3();
            min-width: 215px;

            &:hover {
                @include button_1();
            }
        }
    }
}

