/* ===== comparison-other-pillows-section ===== */
.comparison-other-pillows-section {
    background-color: #F9F5F5;
    padding: 80px 0;

    table {
        border: medium none;
        margin-top: 20px;

        tr {

            th,
            td {
                border: medium none;
                width: 300px;
                padding: 20px 40px 0;

                span.icon-th {
                    width: 35px;
                    height: 35px;
                    background-color: #ffffff;
                    float: right;
                    border-radius: 100%;
                    text-align: center;
                    padding: 8px 0px;
                    margin-top: -10px;
                    margin-left: 10px;
                }
            }

            &:last-child {

                th,
                td {
                    padding-bottom: 40px;
                }
            }

            th,
            td {
                background-color: #E4E4E4;
                color: $primary-color;
                text-align: center;
            }

            th:first-child,
            td:first-child {
                background-color: #EEEEEE;
                text-align: left;
            }

            th {
                font-size: 23px;
                font-family: $moon-bold;
                letter-spacing: 3px !important;
            }

            td {
                font-size: 14px;
                font-family: $primary-font;
            }

            th:first-child {
                font-family: $moon-regular;
            }

            td:first-child {
                font-family: $primary-font;
                font-size: 14px;
                font-weight: 500;
            }

            th:last-child,
            td:last-child {
                background-color: #8486EC;
                color: $white;
                letter-spacing: 1px;
            }
        }
    }
}

