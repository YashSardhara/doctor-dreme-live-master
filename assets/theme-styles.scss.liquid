    margin-bottom: 1em;
}

h3 {
    font-size: 20px;
    margin-bottom: 1.25em;
}

h4 {
    font-size: 17px;
    margin-bottom: 1.5em;
}

h5 {
    font-size:15px;
    margin-bottom: 1.5em;
}

h6 {
    font-size: 1em;
    margin-bottom: 1.5em;
}

p:last-child,
p:last-of-type {
    margin-bottom: 0;
}

small {
    font-size: 12px;
}

/* ----- Table ------- */
table {
    border: 1px solid $border_color_1;
  	min-width:100%;

    td,
    th {
        border-bottom: 1px solid $border_color_1;
        padding: 9px 15px;
    }

    th {
        font-size: 11px;
        text-transform: uppercase;
        font-weight: 700;
    }
}

.btn {
    text-transform: uppercase;
    font-weight: 700;
    letter-spacing: .05em;
    font-size: $font_size + 1;
    line-height: $font_size + 8;
    outline: none;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    @include border-radius(0);
    padding: 8px 15px 6px;
    border: 1px solid rgba(255,255,255,0);
    min-width: 130px;
    max-width: 100%;
    @include transition(all .3s ease);

    &:hover,
    &:focus {
        outline: none;
        box-shadow: none;
    }
}

.btn[disabled] {
    background: $bg_sold_out !important;
    border-color: $bg_sold_out !important;
    color: $sold_out_text !important;
    @include opacity(1);
}

.close {
    @include opacity(1);
    float: none;

    svg {
        width: 17px;
        height: 17px;
    }

    &:hover,
    &:focus {
        @include opacity(1);
    }
}

.fancybox-slide {
    .fancybox-image-wrap {
        backface-visibility: visible;
        -moz-backface-visibility: visible;
        -webkit-backface-visibility: visible;
    }
}

.not_img {
    height: 50vh;
    line-height: 50vh;
    background-color: rgba(35,35,35,0.2);
    width: 100%;
    display: block;
    text-align: center;
    font-size: 24px;
    font-weight: 700;
    color: #fff;
    text-shadow:  0 1px 0 #ccc,
        0 2px 0 #c9c9c9,
        0 3px 0 #bbb,
        0 4px 0 #b9b9b9,
        0 5px 0 #aaa,
        0 6px 1px rgba(0,0,0,.1),
        0 0 5px rgba(0,0,0,.1),
        0 1px 3px rgba(0,0,0,.3),
        0 3px 5px rgba(0,0,0,.2),
        0 5px 10px rgba(0,0,0,.25),
        0 10px 10px rgba(0,0,0,.2),
        0 20px 20px rgba(0,0,0,.15);
}

/* ----- Header ------- */
.header-top {
    .top-message {
        font-size: $font_size;
        font-weight: 500;
        text-transform: uppercase;
        letter-spacing: .05em;
        text-align: center;
        padding: 8px 25px;
        position: relative;

        p {
        a {
            border-bottom: 1px solid rgba(255, 255, 255, 0);
        }
        }
    }

    .close {
        display: inline-block;
        width: 20px;
        height: 20px;
        vertical-align: top;
        position: absolute;
        top: 50%;
        right: -3px;
        @include translateY(-50%);
        margin-top: -.5px;
    }
}

.svg-mb {
    width: 60px;
    height: 50px;
    @include flexbox();
    @include align-items(center);

    a {
        @include inline-flex();
    }

    .icon-search {
        svg {
            position: relative;
            top: -1px;
        }
    }
}

.icon-user {
    svg {
        width: 21px;
        height: 21px;
        stroke-width: 10px;
    }
}

.icon-search {
    @include inline-flex();

    &+.search-form {
        display: none;
    }
}

.cart-icon {
    position: relative;
    padding-right: 10px;
    width: 65px;

    svg {
        stroke: rgba(255, 255, 255, 0);
        width: 22px;
        height: 23px;
    }

    .cartCount {
        position: absolute;
        top: 7px;
        right: -1px;
       min-width: 14px;
    min-height: 18px;
        text-align: center;
        padding: 2px 0px 0 1px;
        @include border-radius(50%);
        z-index: 2;
        font-weight: 500;
    }
}

.icon-nav {
    border: none;
    outline: none;
    padding: 0;
    display: block;
    width: 24px;
    height: 24px;
    position: relative;


    .icon-line {
        display: block;
        width: 24px;
        height: 2px;
        margin: 3px 0;
        position: absolute;
        left: 0;
        right: 0;
        top: 0;
        background-color: {{ settings.color_menu_mb }};

        &:before,
        &:after {
            background-color: {{ settings.color_menu_mb }};
            display: block;
            width: 24px;
            height: 2px;
            margin: 3px 0;
            position: absolute;
            left: 0;
            right: 0;
            content: "";
            @include transition(transform .3s);
            @include transition-delay(.2s);
        }

        &:before {
            top: 5px;
        }

        &:after {
            top: 12px;
        }
    }
}

.close-menu-mb {
    display: none;

    &.menu-open {
        display: block;
        position: fixed;
        background: {{ settings.bg_sale }};
        z-index: 33;
        top: 0;
        right: 0;
        width: 40px;
        height: 40px;

        .icon-line {
            background-color: rgba(255, 255, 255, 0);

            &:before,
            &:after {
                background-color: {{ settings.sale_text }};
                left: 8px;
            }

            &:before {
                @include rotate(45);
                top: 12px;
            }

            &:after {
                @include rotate(-45);
            }
        }
    }
}

