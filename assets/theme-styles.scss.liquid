            min-width: 149px;
            position: relative;
        }
    }

    .view-mode {
        padding: 5px 0;
        @include flexbox();

        .icon-mode {
            min-width: 19px;
            height: 26px;
            display: block;
            border: 1px solid #dcdcdc;
            margin-left: 5px;
            position: relative;
            cursor: pointer;

            &:first-of-type {
                margin-left: 0;
            }

            &:before {
