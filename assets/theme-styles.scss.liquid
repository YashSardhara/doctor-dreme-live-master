                        width: 25%;
                    }
                }
            }

            .slick-arrow {
                @include calc(top, '50% - 22px');
                @include opacity(0);

                &.slick-prev {
                    @include calc(left, '(100% - 1170px)/2 + 50px');
                }

                &.slick-next {
                    @include calc(right, '(100% - 1170px)/2 + 50px');
                }
            }

            &:hover {
                .slick-arrow {
                    @include opacity(1);
                }
            }

            &.four-items,
            &.five-items {
                .video-open-popup {
                    > a {
                        @include calc(left, '25% + 20px');
                    }
                }

                .product-label {
                    right: 25%;
                }

                &:before,
                &:after {
                    width: 25%;
                }

                .slick-arrow {
                    &.slick-prev {
                        left: 25%;
