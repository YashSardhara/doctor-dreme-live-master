    line-height: 50px;
    font-size: 50px;
    font-weight: normal;
}

.bluetext {
    color: #023e88;
}

.primary-button {
    border: 1px solid #fff !important;
    border-radius: 6px;
    display: inline-block;
    font-size: 13px;
    margin-top: 20px;
    font-family: 'gothamRounded-medium';
    font-weight: 600;
    padding: 3px 15px 3px;
    min-width: 120px;
    max-width: 100%;
    text-align: center;
    text-transform: uppercase;
    color: $primary-color;
    @include transition(all 0.4s ease);
    outline: inherit !important;
    box-shadow: inherit !important;
    -webkit-transform: perspective(1px) translateZ(0);
    transform: perspective(1px) translateZ(0);
    -webkit-transition-property: color;
    transition-property: color;
    overflow: hidden;
    background-color: transparent;
    cursor: pointer;

    &::before {
        content: "";
        position: absolute;
        z-index: -1;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        border-radius: 6px;
        background: $primary-color;
        -webkit-transform: scaleX(0);
        transform: scaleX(0);
        -webkit-transform-origin: 0 50%;
        transform-origin: 0 50%;
        -webkit-transition-property: transform;
        transition-property: transform;
        -webkit-transition-duration: 0.3s;
        transition-duration: 0.3s;
        -webkit-transition-timing-function: ease-out;
        transition-timing-function: ease-out;
    }
}

.primary-button-second,
.jdgm-widget-actions-wrapper .jdgm-write-rev-link {
    @extend .primary-button;
   background: #023E85 0% 0% no-repeat padding-box;
    border: 1px solid #023E86;
    border-radius: 6px;
    color: #FCFCFC;
    opacity: 1;
    font-family: 'gothamRounded-medium';
    font-size: 14px;

    &::before {
        background: $secondary-color !important;
    }
}

.secondary-button {
    @extend .primary-button;
    background-color: #083F88!important;
    color: $white !important;


    &::before {
           background: #fff!important;
    border: 1px solid #fff !important;
    
    }
}

.primary-button:hover,
.primary-button.active,
.primary-button:focus {
    color: $white !important;

    &::before {
        -webkit-transform: scaleX(1);
        transform: scaleX(1);
    }
}

.primary-button-second:hover,
.jdgm-widget-actions-wrapper .jdgm-write-rev-link:hover,
.primary-button-second.active,
.jdgm-widget-actions-wrapper .jdgm-write-rev-link.active,
.primary-button-second:focus,
.jdgm-widget-actions-wrapper .jdgm-write-rev-link:focus {
    color: $white !important;

    &::before {
        -webkit-transform: scaleX(1);
        transform: scaleX(1);
    }
}

.secondary-button:hover,
.secondary-button.active,
.secondary-button:focus {
    color: $primary-color !important;

    &::before {
        -webkit-transform: scaleX(1);
        transform: scaleX(1);
    }
}

input.secondary-button:hover,
input.secondary-button.active,
input.secondary-button:focus {
