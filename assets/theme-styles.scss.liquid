/* ----- Product Grid ------- */
.widget-title {
    text-align: center;
    padding-top: 15px;
    margin-bottom: 25px;

    .box-title {
        color: $page_title_color;
        font-size: $font_size + 8;
        letter-spacing: .05em;
        font-weight: 700;
        text-transform: uppercase;
        position: relative;
        margin-bottom: 15px;

        .title {
            padding: 0 24px;
            position: relative;
            z-index: 1;
            background-color: $body_bg;
        }

        &:before {
            position: absolute;
            content: "";
            height: 1px;
            background-color: $border_widget_title;
            @include calc(top, '50% - 0.5px');
            left: 0;
            right: 0;
            width: 100%;
        }

        &+p {
            margin-bottom: 30px;
            line-height: $font_size + 10;
            max-width: 770px;
            margin-left: auto;
            margin-right: auto;
            position: relative;
            top: -2px;
        }
    }

    .view_all {
        display: inline-block;
        text-transform: uppercase;
        font-weight: 600;
        color: $body_color;
        padding-left: 13px;
        position: relative;

        &:before {
            position: absolute;
            content: "";
            top: 3px;
            left: 5px;
            width: 1px;
            height: 14px;
            background-color: #787878;
        }
    }

    &.not-before {
        margin-top: 0;

        .box-title {
            .title {
                background-color: rgba(255, 255, 255, 0);
            }

            &:before {
                display: none;
            }
        }
    }

    &.style-3 {
        text-align: left;
        padding: 0;
        position: relative;

        .box-title {
            border-bottom: 2px solid $border_widget_title;
            padding: 12px 60px 12px  0;

            .title {
                padding: 0;
            }
        }

        .view_all {
            position: absolute;
            top: 14px;
            right: 0;
            margin-top: 0;
            text-transform: capitalize;
            font-weight: normal;
            color: $page_title_color;

            &:before {
                display: none;
            }
        }
    }
}

.image-swap {
    position: relative;

    .images-one,
    .images-two {
        @include transition(all .5s ease);
    }

    .images-two {
        position: absolute !important;
        top: 0;
        left: 0;
        bottom: 0;
        right: 0;
        @include opacity(0);
        width: 100%;
        height: 100%;
        cursor: pointer;
    }
