/* ----- Sub Banner ------- */
.border-bt-hover {
    position: absolute;
    left: 0;
    width: 0;
    bottom: -.3rem;
    height: 2px;
    content: '';
    display: block;
    @include transition(width .3s ease);
}

.banner-item {
    position: relative;

    .img-box {
        text-align: center;
    }

    .banner-title {
        font-size: 16px;
        letter-spacing: .05em;
        font-weight: 700;
        text-transform: uppercase;
        margin: 0;
        display: inline-block;
        position: relative;
    }

    .des {
        font-size: $font_size;
        line-height: $font_size + 10;
    }

    .content-box {
        position: absolute;
        top: 50%;
        left: 50%;
        @include translate(-50%, -50%);
        min-width: 90%;
        text-align: center;
    }

    &:hover {
