/* navigation menu start here */
$black: #333333;
$white: #ffffff;
$light-gray: #F2F2F2;
$primary-color: #3C3C9F;
$secondary-color: #00ACBD;
$content-color: #707070;
$moon-regular: 'moon_2.0regular';
