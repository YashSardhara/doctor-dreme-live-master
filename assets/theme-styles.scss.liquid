.swatch {
    margin-bottom: 13px;

    .header {
        font-size: $font_size;
        font-weight: 500;
        line-height: 20px;
        color: $page_title_color;
        margin-bottom: .5rem;

        em {
            color: $bg_sale;
        }
    }

    input[type=radio],
    .swatch-element .crossed-out {
        display: none;
    }

    .soldout{
        @include opacity(0.4);
    }

    .swatch-element {
        @include inline-flex();
        margin: 0 10px 12px 0;
        position: relative;

        &.soldout {
            label {
                cursor: no-drop;

                &:after {
                    display: none;
                }
            }
        }

        label {
            min-width: 34px;
            height: 34px;
            border: 1px solid $border_color_2;
            margin: 0;
            text-align: center;
            padding: 3px;
            @include border-radius(17px);
            line-height: $font_size + 14;
            color: $color_slick_arrow;
            cursor: pointer;
        }

        .bgImg {
            display: block;
            width: 100%;
            height: 100%;
            @include border-radius(50%);
            background-repeat: no-repeat;
            background-position: center;
            background-size: cover;
        }

        &:not(.color) {
            label {
                &:after {
                    position: absolute;
                    top: 0;
                    left: 0;
                    content: "";
                    width: 100%;
                    height: 100%;
                    border: 2px solid $color_slick_arrow;
                    background-color: rgba(255, 255, 255, 0);
                    z-index: 2;
                    @include opacity(0);
                    visibility: hidden;
                    @include transition(opacity .2s linear);
                    @include border-radius(17px);
                }
            }
        }

        &.color {
            label {
                position: relative;

                &:after {
                    position: absolute;
                    top: 0;
                    left: 0;
                    content: "";
                    width: 100%;
                    height: 100%;
                    border: 4px solid $color_slick_arrow;
                    background-color: rgba(255, 255, 255, 0);
                    z-index: 2;
                    @include opacity(0);
                    visibility: hidden;
                    @include transition(opacity .2s linear);
                    @include border-radius(50%);
                }

                &.swatch_variant_img {
                    &:after {
                        border-width: 3px;
                    }
                }
            }
        }

        input:checked {
            &+label {
                font-weight: 600;

                &:after {
                    @include opacity(1);
                    visibility: visible;
                }
            }
        }
    }

    .tooltip {
        text-align: center;
        background: $background_1;
        color: $color_1;
        bottom: 100%;
        padding: 5px 10px;
        display: block;
        position: absolute;
        z-index: 2;
        min-width: 50px;
        left: 50%;
        @include translateX(-50%);
        margin-bottom: 12px;
        @include opacity(0);
        visibility: hidden;
        @include transition(all .25s ease-out);
        @include box-shadow(2px, 2px, 6px, rgba(0,0,0,0.28));
        font-size: $font_size;
        font-weight: 500;
        letter-spacing: normal;
        pointer-events: none;

        &:before {
            bottom: -16px;
            content: "";
            display: block;
            height: 16px;
            left: 0;
            position: absolute;
            width: 100%;
        }

        &:after {
            border-left: solid transparent 7px;
            border-right: solid transparent 7px;
            border-top: solid $background_1 7px;
            bottom: -7px;
            content: " ";
            height: 0;
            left: 50%;
            @include translateX(-50%);
            position: absolute;
            width: 0;
        }
    }

    .swatch-element:hover .tooltip {
        @include opacity(1);
        visibility: visible;
        pointer-events: auto;
    }
}

