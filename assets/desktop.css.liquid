body {
  max-width: 1600px;
  margin: 0 auto;
}
.product-price--compare {
  text-transform: none;
}

/* account popup */
#guest {
  display: none !important;
}
.faq_info {
  margin-bottom: 3rem;
  text-align: left
}
.faq_info .heading_text {
  width: 100%
}
.ContentClass {
  max-width: 990px;
  margin: 0 auto;
}
.layout--login a img {
  position: relative;
}

.account_popup {
  position: absolute;
  width: 290px;

  right: 0;
  background-color: var(--bg_color1);
  box-shadow: #00000029 0 2px 4px;
  z-index: 99999999;
  text-align: center !important;
  /* opacity         : 0;
    visibility      : hidden; */
  color: #fff;
  transition: max-height 0s 1.5s, opacity 0.4s 1s;
}
.layout--login:hover ~ .account_popup {
  opacity: 1;
  visibility: visible;
}

/* account popup end */
