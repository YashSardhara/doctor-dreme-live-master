<div id="Manage" class="tabcontent">
  <h2 class="account_heading">Saved Address</h2>
  <div class="address--container address__container">
    <div class="account_address">
      {% for address in customer.addresses %}
      <div class="address--table m_addres" id="view_address_{{ address.id }}"> 
        <div class="address--customer">
          <h4 data-item="block-heading">
            {{ address.street }}
            
            {% if address == customer.default_address %}
            <span data-item="paragraph">
              {{ 'customer.addresses.default' | t }}
            </span>
            {% endif %}
          </h4>
          
          
          
          <div class="address--view" data-item="paragraph">
            <p class="address_icon"><span><svg xmlns="http://www.w3.org/2000/svg" width="15.253" height="15.228" viewBox="0 0 15.253 15.228">
              <path id="Shape_650" data-name="Shape 650" d="M228.061,319.026a6.436,6.436,0,0,0-12.833-.682c-.026.226-.025.455-.037.682H214a7.654,7.654,0,0,1,5.089-7.221,4.348,4.348,0,0,1-1.867-3.573,4.22,4.22,0,0,1,1.3-3.154,4.4,4.4,0,1,1,5.607,6.748l.166.066a7.572,7.572,0,0,1,4.636,4.956,6.684,6.684,0,0,1,.293,1.589,1.209,1.209,0,0,0,.026.142v.446Zm-6.432-14.039a3.212,3.212,0,1,0,3.214,3.222A3.224,3.224,0,0,0,221.629,304.987Z" transform="translate(-214 -303.799)"/>
            </svg></span>{{ address.first_name }} {{ address.last_name }}</p>
            <!-- <p>{{ address.company }}</p> -->
