{% comment %}
  Translations
{% endcomment %}
{% assign search_t = 'general.search.search' | t %}
{% assign cart_title_t = 'cart.title' | t %}

{% comment %}
  Variables
{% endcomment %}
{% assign menu = section.settings.menu %}
{% assign logo = section.settings.logo %}
{% assign logo_height = section.settings.logo-height-range %}
{% assign show_search = settings.search--show-in-header %}
{% assign cart_type = settings.cart--type %}
{%- assign mega_menu = section.settings.main_linklist -%}

<header class="d_flex main_header" data-section-id="{{ section.id }}" data-section-type="header" data-js-class="Header" {% if logo %} {% if logo_height <=45 %} data-header-logo-type="inline"{% else %}data-header-logo-type="overflowing"{% endif %} {% endif %}>

  <noscript>
    <div class="header--container">
      <div class="header--left-side ">
        <a class="logo--wrapper" href="{{ routes.root_url }}" title="{{ shop.name }}" itemprop="url">
          {% if section.settings.logo != blank %}
            <img src="{{section.settings.logo  | img_url: 'master'}}" alt="">
          {% else %}
            <div class="logo--text" data-item="section-heading">
              {{ shop.name }}
            </div>
          {% endif %}
        </a>
      </div>
      <div class="header--right-side" data-item="nav-text">
        {% if show_search %}
          <div class="header--search">
            <a href="{{ routes.search_url }}">{{ search_t }}
            </a>
          </div>
        {% endif %}

        {%- comment -%}
          {% render 'layout--login' %}
        {%- endcomment -%}

        <div class="cart-link" data-item="nav-text">
          <a href="{{ routes.cart_url }}">
            {{ cart_title_t }}
          </a>
        </div>
      </div>
    </div>
  </noscript>
  <div class="header_top_content d_flex">
    <div class="header--left-side d_flex">
      {%- comment -%}mobile menu{%- endcomment -%}
      <div class="mobile_menu_div">
        <svg class="icon--root icon--menu" viewBox="0 0 22 16">
