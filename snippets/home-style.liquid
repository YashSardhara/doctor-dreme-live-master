  .shop_btn_toggle span.switch_btn .switch {
  position: relative;
  display: inline-block;
  width: 60px;
  height: 35px;
}

 .shop_btn_toggle span .switch input { 
  opacity: 0;
  width: 0;
  height: 0;
}

 .shop_btn_toggle span .slider {
  position: absolute;
  cursor: pointer;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: transparent;
  -webkit-transition: .4s;
  transition: .4s;border: 1px solid #d5d5d5;
}

 .shop_btn_toggle span .slider:before {
  position: absolute;
  content: "";
  height: 26px;
  width: 26px;
  left: 29px;
  bottom: 4px;
  background-color: #f90a10;
  -webkit-transition: .4s;
  transition: .4s; border: 1px solid #d5d5d5;
}

 .shop_btn_toggle span input:checked + .slider {
    background-color: transparent;
    border: 1px solid #d5d5d5;
}

 .shop_btn_toggle span input:focus + .slider {
  box-shadow: 0 0 1px #2196F3;
}

  .shop_btn_toggle span input:checked + .slider:before {
  -webkit-transform: translateX(26px);
  -ms-transform: translateX(26px);
  transform: translateX(-26px);
}
