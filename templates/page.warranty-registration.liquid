<div class="warranty_container">
  <div class="warranty-page row">
    <span class="d_flex heading_div warranty_head">
      <h2 class="featured-collections--heading heading_text" data-item="section-heading"> Warranty Registration </h2>
    </span>
  </div>
  <div class="form_head_p">
    <p class="l_para">Your Details</p>
    {% unless customer %}
    <p class="log_in_p">Already have an account?&nbsp;&nbsp;<a href="javascript:void(0)" onclick="openLoginForm()">Log
        In</a>
      {% endunless %}
    </p>
  </div>
  <form id="warrenty_form" method="POST" enctype="multipart/form-data">
    <div class="warrenty-form">
      <div class="form-row">
        <div class="form-group col-md-6 did-floating-box">
          <input class="form-control fname buttonstyle" type="text"
            value="{% if customer %}{{ customer.first_name }}{% endif %}" id="first-name" name="first_name"
            placeholder=" " autocapitalize="words" v-on:keypress="isLetter($event)" required>
          <label class="did-floating-label">First Name</label>
          <span id="first_name_error" class="help-inline text-danger"></span>
        </div>
        <div class="form-group col-md-6 did-floating-box">
          <input class="form-control lname buttonstyle" type="text"
            value="{% if customer %}{{  customer.last_name  }}{% endif %}" id="last-name" name="last_name"
            autocapitalize="words" placeholder=" " required>
          <label class="did-floating-label">Last Name</label>
          <span id="last_name_error" class="help-inline text-danger"></span>
        </div>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6 did-floating-box">
          <input type="email" class="form-control emailadd buttonstyle"
            value="{% if customer %}{{  customer.email  }}{% endif %}" name="email" id="Email" autocorrect="off"
            autocapitalize="off" placeholder=" " required>
          <label class="did-floating-label">Email ID</label>
          <span id="email_error" class="help-inline text-danger"></span>
        </div>
        <div class="form-group col-md-6 did-floating-box">
          <input class="form-control buttonstyle"
            value="{% if customer %}{{ customer.default_address.phone }}{% endif %}" type="tel" name="mobile"
            id="contactFormPhone" pattern="[0-9]{10}" maxlength="10" placeholder=" " required>
          <label class="did-floating-label">Mobile No</label>
          <span id="mobile_error" class="help-inline text-danger"></span>
        </div>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6">
          <div class="did-floating-label-content-war">
            <select id="address_state" name="state" class="did-floating-select"
              onclick="this.setAttribute('value', this.value);" onchange="this.setAttribute('value', this.value);"
              value="">
              {% comment %} {% if customer %}
              <option value="{{ customer.default_address.province  }}" selected>{{ customer.default_address.province }}
              </option>
              {% endif %} {% endcomment %}
              <option value="" selected disabled></option>
            </select>
            <label class="did-floating-label-war">State</label>
          </div>
        </div>
        <div class="form-group col-md-6">
          <div class="did-floating-label-content-war">
            <select id="address_city" name="city" class="did-floating-select"
              onclick="this.setAttribute('value', this.value);" onchange="this.setAttribute('value', this.value);"
              value="">
              <option value="" selected disabled></option>
            </select>
            <label class="did-floating-label-war">City</label>
          </div>
        </div>
      </div>
      <div class="form-row">
        <div class="col-md-3 col-sm-12  warr__radio__cont">
          <div class="radio_online_off">
            <div class="col-md-3 war_radio_box">
              <input type="radio" class="form-control" id="war_on_click" name="warranty" value="OnlinePurchase"
                checked>Online Purchase
            </div>
            <div class="col-md-3 war_radio_box">
              <input type="radio" class="form-control" id="war_of_click" name="warranty" value="offlinePurchase">Offline
              Purchase
            </div>
          </div>
          <div class=" form-group radio_online_off_1">
            <div class="col-3 war__offline" id="warOffline">
              <div class="did-floating-label-content-war">
                <select class="did-floating-select" id="Offline-purchase" name="offline"
                  onclick="this.setAttribute('value', this.value);" onchange="this.setAttribute('value', this.value);"
                  value="">
                  <option value="" selected disabled></option>
                  <option value="Homeretail">Home Retail Store</option>
                  <option value="offother">Other</option>
                </select>
                <label class="did-floating-label-war">Offine Purchase Source</label>
                <span id="offline_error" class="help-inline text-danger"></span>
              </div>
            </div>
            <div class="col-3 war__online" id="onlinePurchase">
              <div class="did-floating-label-content-war">
                <select class="did-floating-select" id="online-purchase" name="online"
                  onclick="this.setAttribute('value', this.value);" onchange="this.setAttribute('value', this.value);"
                  value="">
                  <option value="" selected disabled></option>
                  <option value="Amazon">Amazon</option>
                  <option value="flipkart">Flipkart</option>
                  <option value="pepperfry">Pepperfry</option>
                  <option value="Doctor Dreams">Doctor Dreams</option>
                  <option value="onother">Other</option>
                </select>
                <label class="did-floating-label-war">Online Purchase Source</label>
                <span id="online_error" class="help-inline text-danger"></span>
              </div>
            </div>
          </div>
          <div class="form-group col-md-6 did-floating-box dd_bottom__remove">
            <input type="date" name="date_of_purchase" id="warrantyDop" placeholder="dd-mm-yyyy"
              class=" form-control buttonstyle" />
            <label class="did-floating-label">Date of purchase</label>
            <img class="date_svg-img"
              src="https://cdn.shopify.com/s/files/1/0248/7766/2271/files/Vector_1.svg?v=1682518087" alt="">
            <span id="date_of_purchase_error" class="help-inline text-danger"></span>
          </div>
        </div>

      </div>
